<div class="row justify-content-center mb-3">
  <div class="col-md-8">
    <%= render object: @level, partial: "card", as: :level %>
  </div>
  <div class="col-md-4">
    <h5>Models:</h5>
    <hr>
    <h6>Plate</h6>
    <hr>
    <p>id: integer</p>
    <p>color: string</p>
    <p>fancy: boolean</p>
  </div>
</div>
<div class="">
  <h5>Your Query</h5>
  <p>
    <%= @query %> was
    <% if @res %>
    correct!
    <% else %>
    incorrect!
      <% if @collection_returned %>
        <p>You returned a collection of Plates</p>
      <% end %>
    <% end %>
  </p>
  <%= link_to "Back", level_path(@level.id) %>


</div>
